  <attribute name="Script" type="STRING">PROCEDURE update_TEST_INCR(
	IN  inK1			INTEGER,
	IN  inK2			VARCHAR(10),
	IN  inC3			VARCHAR(255),
	IN  inC4			DECIMAL(10,2),
	OUT result			CURSOR(SEQ INTEGER, K1 INTEGER, K2 VARCHAR, C3 VARCHAR, C4 DECIMAL(10,2), ACTIVITY VARCHAR(1), SDT TIMESTAMP)
)
BEGIN

	BEGIN INDEPENDENT TRANSACTION
		UPDATE /shared/ASAssets/CacheManagement/CacheFrameworkSample/Oracle_SampleSOR/SOR/TEST_INCR
			SET C3 = inC3,
				C4 = inC4,
				SDT = CURRENT_TIMESTAMP,
				ACTIVITY = &apos;U&apos;
			WHERE K1 = inK1
			  AND K2 = inK2;
	COMMIT;
	END;

	OPEN result FOR SELECT { option no_data_cache } * From /shared/ASAssets/CacheManagement/CacheFrameworkSample/Oracle_SampleSOR/SOR/TEST_INCR ORDER BY K1,K2,SDT;

END</attribute>
