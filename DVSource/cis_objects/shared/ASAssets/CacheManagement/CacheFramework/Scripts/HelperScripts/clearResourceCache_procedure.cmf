  <attribute name="Script" type="STRING">/* clearResourceCache:

	This script is used to clear the resource cache.

	Modified Date:	Modified By:		CIS Version:	Reason:
	09/25/2014		Mike Tinius			6.2.6			Created new to make cache framework generic. 
*/
PROCEDURE clearResourceCache(
	IN resourcePath			VARCHAR(4096),
	IN resourceType			VARCHAR
)
BEGIN
	DECLARE moduleName		VARCHAR DEFAULT &apos;clearResourceCache&apos;;
	DECLARE requestXML		XML;
	DECLARE responseXML		XML;
	DECLARE fault			XML;
	DECLARE ex				EXCEPTION;

	SET requestXML = 
&apos;&lt;resource:clearResourceCache xmlns:resource=&quot;http://www.compositesw.com/services/system/admin/resource&quot;&gt;
  &lt;resource:path&gt;&apos;|| CAST(XMLTEXT(resourcePath) AS LONGVARCHAR) ||&apos;&lt;/resource:path&gt;
  &lt;resource:type&gt;&apos;|| CAST(XMLTEXT(resourceType) AS LONGVARCHAR) ||&apos;&lt;/resource:type&gt;
&lt;/resource:clearResourceCache&gt;&apos;;

	CALL /services/webservices/system/admin/resource/operations/clearResourceCache(requestXML, responseXML, fault);
	IF (fault IS NOT NULL) THEN
		RAISE ex VALUE moduleName||&apos;: Error=&apos;||CAST(fault AS LONGVARCHAR);
	END IF;
END</attribute>
